<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <AssemblyName>WPILibSharp.Oblog</AssemblyName>
    <RootNamespace>WPILib.Oblog</RootNamespace>
    <NoWarn>1701;1702;1591;CA1303</NoWarn>
  </PropertyGroup>

  <PropertyGroup>
    <TreatWarningsAsErrors>false</TreatWarningsAsErrors>
    <WarningsAsErrors />
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\wpilibsharp\wpilibsharp.csproj" />
    <ProjectReference Include="..\..\wpiutil\wpiutil.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Update="Nullable" Version="1.3.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

</Project>
